    <dce:subject>QoS</dce:subject>
    <dce:subject>constraint programming</dce:subject>
    <dce:subject>discovery</dce:subject>
    <dce:subject>matchmaking</dce:subject>
    <dce:subject>non-functional</dce:subject>
    <dce:subject>optimization</dce:subject>
    <dce:subject>performance</dce:subject>
