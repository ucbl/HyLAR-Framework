    <dce:subject>Crowd sourcing</dce:subject>
    <dce:subject>Exploratory search</dce:subject>
    <dce:subject>Information Seeking</dce:subject>
    <dce:subject>Multi-domain search</dce:subject>
    <dce:subject>Search engine</dce:subject>
